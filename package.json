{
	"name": "aec-demo-mac",
	"private": true,
	"version": "0.3.0",
	"type": "module",
	"scripts": {
		"dev": "cp .env.dev .env && vite dev",
		"build": "vite build",
		"dev:static": "cp .env.static .env && BUILD_TARGET=static vite dev",
		"build:static": "cp .env.static .env && BUILD_TARGET=static vite build",
		"build:file": "cp .env.file .env && BUILD_TARGET=file vite build",
		"dev:tauri": "pnpm tauri dev",
		"build:tauri": "cp .env.tauri .env && pnpm tauri build",
		"tauri": "tauri",
		"preview": "vite preview",
		"prepare": "svelte-kit sync || echo ''",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"typegen": "npx pocketbase-typegen --env --out ./src/lib/pocketbase/pocketbase-types.ts",
		"lint": "eslint . && prettier --check .",
		"format": "prettier --write ."
	},
	"dependencies": {
		"@tauri-apps/api": "^2",
		"@tauri-apps/plugin-opener": "^2",
		"pocketbase": "^0.26.2"
	},
	"devDependencies": {
		"@eslint/compat": "^1.2.5",
		"@eslint/js": "^9.18.0",
		"@sveltejs/adapter-auto": "^6.0.0",
		"@sveltejs/adapter-static": "^3.0.9",
		"@sveltejs/kit": "^2.22.0",
		"@sveltejs/vite-plugin-svelte": "^6.0.0",
		"@tauri-apps/cli": "^2",
		"cross-env": "^7.0.3",
		"eslint": "^9.18.0",
		"eslint-config-prettier": "^10.0.1",
		"eslint-plugin-svelte": "^3.0.0",
		"globals": "^16.0.0",
		"prettier": "^3.4.2",
		"prettier-plugin-svelte": "^3.3.3",
		"svelte": "^5.0.0",
		"svelte-check": "^4.0.0",
		"typescript": "^5.0.0",
		"typescript-eslint": "^8.20.0",
		"vite": "^7.0.4"
	},
	"pnpm": {
		"onlyBuiltDependencies": [
			"esbuild"
		]
	}
}
